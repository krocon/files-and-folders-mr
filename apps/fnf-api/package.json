{
  "name": "fnf-api",
  "version": "2.0.0",
  "description": "NestJS API for Files and Folders",
  "private": false,
  "scripts": {
    "prebuild": "node ../../update-version.js",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "test-fn": "jest --testPathPattern=\".*fn\\.spec\\.ts$\" --runInBand",
    "test-gateway": "jest --testPathPattern=\".*gateway\\.spec\\.ts$\" --runInBand",
    "test-controller": "jest --testPathPattern=\".*controller\\.spec\\.ts$\" --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepush": "node ../../update-version.js"
  },
  "dependencies": {
    "7zip-bin": "^5.2.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/websockets": "^10.0.0",
    "@types/js-yaml": "^4.0.9",
    "axios": "^1.11.0",
    "chokidar": "^4.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "fs-extra": "^11.3.0",
    "js-yaml": "^4.1.0",
    "micromatch": "^4.0.8",
    "nest-winston": "^1.10.2",
    "ngx-socket-io": "^4.8.5",
    "node-7z": "^3.0.0",
    "node-stream-zip": "^1.15.0",
    "raw-body": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0",
    "xterm": "5.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
